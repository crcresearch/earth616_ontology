@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .

# --- SCODOC ---
@prefix scodoc: <https://schema.e616.crc.nd.edu/nd/scodoc/ont/> .
@prefix : <https://schema.e616.crc.nd.edu/nd/scodoc/ont/> .
@base <https://schema.e616.crc.nd.edu/nd/scodoc/ont/> .

# Classes
scodoc:SupplyChainEvent rdfs:subClassOf scodoc:Event ;
    rdfs:label "Supply Chain Event"@en ;
    rdfs:comment "An event occurring in a supply chain process."@en .

scodoc:TransferEvent rdfs:subClassOf scodoc:SupplyChainEvent ;
    rdfs:label "Transfer Event"@en ;
    rdfs:comment "An event involving the transfer of goods or materials."@en .

scodoc:TransformationEvent rdfs:subClassOf scodoc:SupplyChainEvent ;
    rdfs:label "Transformation Event"@en ;
    rdfs:comment "An event where goods or materials undergo a transformation process."@en .

scodoc:CustodyChangeEvent rdfs:subClassOf scodoc:SupplyChainEvent ;
    rdfs:label "Custody Change Event"@en ;
    rdfs:comment "An event where the custody of goods or materials changes."@en .

scodoc:OwnershipChangeEvent rdfs:subClassOf scodoc:SupplyChainEvent ;
    rdfs:label "Ownership Change Event"@en ;
    rdfs:comment "An event where the ownership of goods or materials changes."@en .

scodoc:TransportEvent rdfs:subClassOf scodoc:TransferEvent ;
    rdfs:label "Transport Event"@en ;
    rdfs:comment "An event involving the transportation of goods or materials."@en .

scodoc:ObservationEvent rdfs:subClassOf scodoc:SupplyChainEvent ;
    rdfs:label "Observation Event"@en ;
    rdfs:comment "An event where an observation or measurement is made on goods or materials."@en .

scodoc:SupplyChainRole rdfs:subClassOf scodoc:ParticipantRole ;
    rdfs:label "Supply Chain Role"@en ;
    rdfs:comment "A role played by an entity in a supply chain event."@en .

scodoc:SourceRole rdfs:subClassOf scodoc:SupplyChainRole ;
    rdfs:label "Source Role"@en ;
    rdfs:comment "The role of the entity from which goods or materials originate."@en .

scodoc:TargetRole rdfs:subClassOf scodoc:SupplyChainRole ;
    rdfs:label "Target Role"@en ;
    rdfs:comment "The role of the entity to which goods or materials are destined."@en .

scodoc:EquipmentRole rdfs:subClassOf scodoc:SupplyChainRole ;
    rdfs:label "Equipment Role"@en ;
    rdfs:comment "The role of equipment or machinery used in an event."@en .

scodoc:SourceCustodianRole rdfs:subClassOf scodoc:SupplyChainRole ;
    rdfs:label "Source Custodian Role"@en ;
    rdfs:comment "The role of the entity that has custody of goods or materials before an event."@en .

scodoc:TargetCustodianRole rdfs:subClassOf scodoc:SupplyChainRole ;
    rdfs:label "Target Custodian Role"@en ;
    rdfs:comment "The role of the entity that gains custody of goods or materials after an event."@en .

scodoc:SourceOwnerRole rdfs:subClassOf scodoc:SupplyChainRole ;
    rdfs:label "Source Owner Role"@en ;
    rdfs:comment "The role of the entity that owns goods or materials before an event."@en .

scodoc:TargetOwnerRole rdfs:subClassOf scodoc:SupplyChainRole ;
    rdfs:label "Target Owner Role"@en ;
    rdfs:comment "The role of the entity that gains ownership of goods or materials after an event."@en .

scodoc:BeingShippedRole rdfs:subClassOf scodoc:SupplyChainRole ;
    rdfs:label "Being Shipped Role"@en ;
    rdfs:comment "The role of goods or materials being shipped or transported."@en .

scodoc:BeingMeasuredRole rdfs:subClassOf scodoc:SupplyChainRole ;
    rdfs:label "Being Measured Role"@en ;
    rdfs:comment "The role of goods or materials being observed or measured."@en .

scodoc:Agent rdfs:subClassOf scodoc:Agent ;
    rdfs:label "Agent"@en ;
    rdfs:comment "An entity participating in a supply chain event, such as a person, organization, or system."@en .

scodoc:SupplyChainSpatiotemporalExtent rdfs:subClassOf scodoc:SpatioTemporalExtent ;
    rdfs:label "Supply Chain Spatiotemporal Extent"@en ;
    rdfs:comment "Represents the spatial and temporal extent of a supply chain event."@en .

scodoc:TransformationActivity rdfs:subClassOf owl:Thing ;
    rdfs:label "Transformation Activity"@en ;
    rdfs:comment "An activity or process that transforms goods or materials."@en .

# Properties
scodoc:providesRole rdfs:subPropertyOf scodoc:providesParticipantRole ;
    rdfs:label "provides role"@en ;
    rdfs:domain scodoc:SupplyChainEvent ;
    rdfs:range scodoc:SupplyChainRole .

scodoc:assumesRole rdfs:subPropertyOf scodoc:assumesRole ;
    rdfs:label "assumes role"@en ;
    rdfs:domain scodoc:Agent ;
    rdfs:range scodoc:SupplyChainRole .

scodoc:hasSTETE rdfs:subPropertyOf scodoc:hasSpatioTemporalExtent ;
    rdfs:label "has spatiotemporal extent"@en ;
    rdfs:domain scodoc:SupplyChainEvent ;
    rdfs:range scodoc:SupplyChainSpatiotemporalExtent .

scodoc:hasTemporalExtent rdfs:subPropertyOf scodoc:hasSpatioTemporalExtent ;
    rdfs:label "has temporal extent"@en ;
    rdfs:domain scodoc:SupplyChainEvent ;
    rdfs:range scodoc:TemporalExtent .

scodoc:hasContainedInRelation rdfs:subPropertyOf scodoc:subEventOf ;
    rdfs:label "has contained in relation"@en ;
    rdfs:domain scodoc:SupplyChainEvent ;
    rdfs:range scodoc:ContainedInRelation .

scodoc:hasFurtherDocumentation rdfs:subPropertyOf scodoc:hasInformationObject ;
    rdfs:label "has further documentation"@en ;
    rdfs:domain scodoc:SupplyChainEvent ;
    rdfs:range scodoc:DocumentationSource .

scodoc:occursAt rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:label "occurs at"@en ;
    rdfs:domain scodoc:TransformationEvent ;
    rdfs:range scodoc:TransformationActivity .

scodoc:hasPreCondition rdfs:subPropertyOf scodoc:subEventOf ;
    rdfs:label "has precondition"@en ;
    rdfs:domain scodoc:SupplyChainEvent ;
    rdfs:range scodoc:SupplyChainEvent .

scodoc:hasPostCondition rdfs:subPropertyOf scodoc:subEventOf ;
    rdfs:label "has postcondition"@en ;
    rdfs:domain scodoc:SupplyChainEvent ;
    rdfs:range scodoc:SupplyChainEvent .

scodoc:producedAt rdfs:subPropertyOf scodoc:subEventOf ;
    rdfs:label "produced at"@en ;
    rdfs:domain scodoc:ObservationEvent ;
    rdfs:range scodoc:SupplyChainEvent .

scodoc:hasDocument rdf:type owl:ObjectProperty ;
                   rdfs:domain scodoc:DocumentationSource ;
                   rdfs:range scodoc:ScdigitaldocumentDocument ;
                   rdfs:isDefinedBy <https://schema.e616.crc.nd.edu/nd/scodoc/ont/> ;
                   rdfs:label "hasDocument" .

# Additional classes and properties
scodoc:ContainedInRelation rdfs:subClassOf owl:Thing ;
    rdfs:label "Contained In Relation"@en ;
    rdfs:comment "Represents a containment relationship between events."@en .

scodoc:DocumentationSource rdfs:subClassOf scodoc:InformationObject ;
    rdfs:label "Documentation Source"@en ;
    rdfs:comment "A source of documentation or information related to an event."@en .

# Axioms and restrictions
scodoc:SupplyChainEvent rdfs:subClassOf [
    rdf:type rdfs:Datatype ;
    owl:equivalentClass [
        rdf:type rdfs:Restriction ;
        owl:onProperty scodoc:providesRole ;
        owl:someValuesFrom scodoc:SupplyChainRole
    ]
] .

scodoc:SupplyChainEvent rdfs:subClassOf [
    rdf:type rdfs:Datatype ;
    owl:equivalentClass [
        rdf:type rdfs:Restriction ;
        owl:onProperty scodoc:hasSTETE ;
        owl:someValuesFrom scodoc:SupplyChainSpatiotemporalExtent
    ]
] .