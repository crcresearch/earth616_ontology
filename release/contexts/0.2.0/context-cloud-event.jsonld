{
  "@version": 1.1,

  "id":   "@id",
  "type": "@type",

  "prov":  "http://www.w3.org/ns/prov#",
  "dct":   "http://purl.org/dc/terms/",
  "schema": "http://schema.org/",
  "xsd":   "http://www.w3.org/2001/XMLSchema#",

  "activity_start_time": {
    "@id": "prov:startedAtTime",
    "@type": "xsd:dateTime"
  },
  "activity_end_time": {
    "@id": "prov:endedAtTime",
    "@type": "xsd:dateTime"
  },
  "creator": {
    "@id": "prov:wasAssociatedWith",
    "@type": "@id"
  },
  "entity": {
    "@id": "prov:generated",
    "@type": "@id"
  },
  "derived_from": {
    "@id": "prov:used",
    "@type": "@id",
    "@container": "@set"
  },

  "content_type": { "@id": "dct:format" },
  "name":         { "@id": "schema:name" },
  "sensitivity":  { "@id": "dct:accessRights", "@container": "@set" },
  "timestamp":    { "@id": "prov:generatedAtTime", "@type": "xsd:dateTime" },
  "content":      { "@id": "schema:encoding" },
  "uri":          { "@id": "prov:atLocation", "@type": "@id" },
  "source":       { "@id": "prov:wasAttributedTo", "@type": "@id" }
}