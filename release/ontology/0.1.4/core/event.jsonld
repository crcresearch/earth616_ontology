            "rdfs:isDefinedBy": {
                "@id": "dscdo:modules/core/event"
            },
            "rdfs:comment": {
                "@language": "en",
                "@value": "An entity participating in an event, such as a person, organization, or system."
            },
            "rdfs:label": {
                "@language": "en",
                "@value": "Agent"
            },
            "@type": "rdfs:Class"
        },
        {
