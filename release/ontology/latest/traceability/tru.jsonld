            "@id": "dscdo:observationIntegration",
            "skos:example": {
                "@language": "en",
                "@value": "\n{\n  \"@context\": {\n    \"@vocab\": \"https://vocab.earth616.localhost/ont/\",\n    \"schema\": \"http://schema.org/\",\n    \"Observation\": \"schema:Observation\",\n    \"observationAbout\": \"schema:observationAbout\",\n    \"observationDate\": \"schema:observationDate\",\n    \"measuredProperty\": \"schema:measuredProperty\",\n    \"value\": \"schema:value\"\n  },\n  \"@graph\": [\n    {\n      \"@type\": \"ObservationEvent\",\n      \"@id\": \"https://vocab.earth616.localhost/ont/examples/events/mass-measurement-001\",\n      \"startedAtTime\": \"2024-03-15T08:45:00Z\",\n      \"observationAbout\": \"https://vocab.earth616.localhost/ont/examples/tru/steel-batch-001\",\n      \"observationDate\": \"2024-03-15T08:45:00Z\",\n      \"hasObservationResult\": {\n        \"@type\": \"Observation\",\n        \"@id\": \"https://vocab.earth616.localhost/ont/examples/observations/mass-obs-001\",\n        \"observationAbout\": \"https://vocab.earth616.localhost/ont/examples/tru/steel-batch-001\",\n        \"observationDate\": \"2024-03-15T08:45:00Z\",\n        \"measuredProperty\": \"mass\",\n        \"value\": {\n          \"@type\": \"QuantityOfMaterial\",\n          \"value\": 25.5,\n          \"unitCode\": \"KGM\",\n          \"unitText\": \"kilograms\",\n          \"hasMeasurementMethod\": \"industrial_scale_weighing\",\n          \"measurementObservation\": \"https://vocab.earth616.localhost/ont/examples/observations/mass-obs-001\"\n        },\n        \"observationMethod\": \"digital_industrial_scale\",\n        \"marginOfError\": 0.1\n      }\n    }\n  ]\n}\n"
            }
        },
        {
