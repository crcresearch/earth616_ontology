:TransformationEvent rdfs:seeAlso ammo:AMProcess ;
    skos:closeMatch ammo:AMProcess,
        ammo:MaterialTransformation ;
    skos:note "Both represent material transformation processes. AMMO focuses on additive manufacturing processes (PowderBed, MaterialExtrusion, etc.) while TRU TransformationEvent is more general for supply chain operations."@en .

:currentState a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "current state"@en ;
    rdfs:comment "The current state of an entity at the present time. Each entity has exactly one current state."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    rdfs:range :State ;
    skos:definition "Links entities to their present state"@en .

:describes a owl:ObjectProperty ;
    rdfs:label "describes"@en ;
    dcterms:source ammo:describes ;
    rdfs:comment "Indicates that a design provides the blueprint or specification for a physical or conceptual entity."@en ;
    rdfs:domain :Design ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    skos:definition "Links a design to the entities it specifies or blueprints"@en ;
    skos:exactMatch ammo:describes ;
    skos:note "Derived from AMMO describes property linking designs to components"@en .

:fromState a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "from state"@en ;
    dcterms:source ammo:fromState ;
    rdfs:comment "The initial state before a transition occurs. Each transition has exactly one source state."@en ;
    rdfs:domain :Transition ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    rdfs:range :State ;
    skos:definition "Links transitions to their source states"@en ;
    skos:exactMatch ammo:fromState ;
    skos:note "Derived from AMMO fromState with functional property constraint"@en .

:hasComponent a owl:ObjectProperty ;
    rdfs:label "has component"@en ;
    dcterms:source ammo:hasComponent ;
    rdfs:comment "Indicates that a designed physical system contains a designed component as a functional part."@en ;
    rdfs:domain :DesignedPhysicalSystem ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:range :DesignedComponent ;
    rdfs:subPropertyOf :hasDirectPart ;
    skos:definition "Specialized relationship linking systems to their functional components"@en ;
    skos:exactMatch ammo:hasComponent ;
    skos:note "Derived from AMMO hasComponent for system-component relationships"@en .

:hasDesignSpecification a owl:ObjectProperty ;
    rdfs:label "has design specification"@en ;
    dcterms:source ammo:hasProperPart ;
    rdfs:comment "Links a design to one or more detailed specifications that define its requirements and constraints."@en ;
    rdfs:domain :Design ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:range :DesignSpecification ;
    skos:definition "Connects a design to the formal specifications that detail its implementation requirements"@en ;
    skos:note "Derived from AMMO hasProperPart relationship between Design and DesignSpecification"@en ;
    skos:relatedMatch ammo:hasProperPart .

:isDirectPartOf a owl:ObjectProperty ;
    rdfs:label "is direct part of"@en ;
    rdfs:comment "Indicates that this physical object is an immediate part of another physical object."@en ;
    rdfs:domain :PhysicalObject ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:range :PhysicalObject ;
    rdfs:subPropertyOf :isProperPartOf ;
    owl:inverseOf :hasDirectPart ;
    skos:definition "Inverse of hasDirectPart for immediate containment relationships"@en .

:isProperPartOf a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:label "is proper part of"@en ;
    rdfs:comment "Indicates that this physical object is a proper part of another physical object."@en ;
    rdfs:domain :PhysicalObject ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:range :PhysicalObject ;
    owl:inverseOf :hasProperPart ;
    skos:definition "Inverse of hasProperPart, indicating containment within a larger whole"@en .

:specifies a owl:ObjectProperty ;
    rdfs:label "specifies"@en ;
    dcterms:source ammo:specifies ;
    rdfs:comment "Indicates that a design specification defines requirements or constraints for a particular entity."@en ;
    rdfs:domain :DesignSpecification ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    skos:definition "Links specifications to the entities they define requirements for"@en ;
    skos:exactMatch ammo:specifies ;
    skos:note "Derived from AMMO specifies property linking DesignSpecification to components"@en .

:toState a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "to state"@en ;
    dcterms:source ammo:toState ;
    rdfs:comment "The target state after a transition occurs. Each transition has exactly one target state."@en ;
    rdfs:domain :Transition ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    rdfs:range :State ;
    skos:definition "Links transitions to their target states"@en ;
    skos:exactMatch ammo:toState ;
    skos:note "Derived from AMMO toState with functional property constraint"@en .

:transitionDuring a owl:ObjectProperty ;
    rdfs:label "transition during"@en ;
    rdfs:comment "The time interval during which a state transition occurs."@en ;
    rdfs:domain :Transition ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    rdfs:range time:ProperInterval ;
    rdfs:subPropertyOf time:hasTime .

default1:TransformationEvent a owl:Class ;
    rdfs:label "Transformation Event"@en ;
    rdfs:comment "An event representing the transformation of materials into new products or components."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:subClassOf default1:SupplyChainEvent ;
    skos:definition "An event that records the conversion of input materials into different output products through manufacturing or processing"@en .

default1:containerSbom a owl:ObjectProperty ;
    rdfs:label "container SBOM"@en ;
    dcterms:subject "SBOM"@en,
        "container security"@en,
        "software supply chain"@en ;
    rdfs:comment "Links a workflow execution to the Software Bill of Materials (SBOM) describing the container runtime environment and its software components."@en ;
    rdfs:domain default1:InformationExtractionWorkflow ;
    rdfs:range spdx:Sbom ;
    rdfs:seeAlso <https://www.cisa.gov/sbom> ;
    rdfs:subPropertyOf prov:used ;
    skos:broader "software provenance pattern"@en ;
    skos:definition "A property that associates a containerized workflow with its SBOM, providing full software provenance for compliance and security analysis."@en ;
    skos:example "An AI document processing workflow references the SBOM of its container image containing Python, transformers library, and other dependencies."@en .

default1:dependsOnPackage a owl:ObjectProperty ;
    rdfs:label "depends on package"@en ;
    rdfs:comment "Indicates that a software agent or workflow depends on a specific software package for its execution."@en ;
    rdfs:domain [ owl:unionOf ( prov:SoftwareAgent default1:InformationExtractionWorkflow ) ] ;
    rdfs:range spdx:Package ;
    rdfs:subPropertyOf prov:used ;
    skos:definition "Establishes explicit dependencies between DSCDO workflows/agents and SPDX software packages."@en .

<https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/ammo-alignment/steel-powder-batch> a :TraceableResourceUnit ;
    rdfs:label "Steel Powder Batch for Additive Manufacturing"@en ;
    schema:additionalProperty [ a schema:PropertyValue ;
            schema:propertyID "AMMO_Material_Type" ;
            schema:value "Alloy" ;
            rdfs:seeAlso ammo:Alloy ],
        [ a schema:PropertyValue ;
            schema:description "Aligned with AMMO Material concept for additive manufacturing applications" ;
            schema:propertyID "AMMO_Alignment" ;
            schema:value "ammo:Material" ] ;
    rdfs:comment "Example TRU representing steel powder batch aligned with AMMO material concepts"@en ;
    rdfs:seeAlso ammo:DesignedArtifact ;
    skos:exactMatch ammo:Material ;
    :consistsOf [ a :QuantityOfMaterial ;
            schema:unitCode "KGM" ;
            schema:unitText "kilograms" ;
            schema:value 50.0 ;
            :hasMeasurementMethod "precision_powder_weighing" ;
            :measuredProperty "mass" ;
            :ofMaterialType <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/materials/steel-316l-powder> ] ;
    :hasBatchNumber "AM-STEEL-316L-2024-001" ;
    :hasIdentifier [ a :Identifier ;
            schema:name "AM Steel Powder Batch Identifier" ;
            schema:propertyID "AdditiveManufacturingBatch" ;
            schema:value "AM-STEEL-316L-2024-001" ] ;
    :hasShelfLife "P1Y" .

<https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/hierarchy/guidance-processor> a :ComponentTRU ;
    rdfs:label "Guidance Processor Component"@en ;
    dcterms:source ammo:DesignedComponent ;
    rdfs:comment "Traceable guidance processor component"@en ;
    skos:exactMatch ammo:DesignedComponent ;
    :hasFunction "primary navigation computation" ;
    :hasIdentifier [ a :Identifier ;
            schema:name "Guidance Processor Serial Number" ;
            schema:propertyID "ComponentSerial" ;
            schema:value "GP-2024-001-A" ] ;
    :isComponentOf <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/hierarchy/missile-guidance-system> ;
    :playsRole [ a :ComponentRole ;
            rdfs:label "Primary Processor Role" ;
            rdfs:comment "Handles main computational tasks for guidance" ] .

<https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/hierarchy/missile-guidance-system> a :SystemTRU ;
    rdfs:label "Missile Guidance System TRU"@en ;
    dcterms:source ammo:DesignedPhysicalSystem ;
    rdfs:comment "Complete guidance system with traceable component hierarchy"@en ;
    skos:exactMatch ammo:DesignedPhysicalSystem ;
    :hasComponent <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/hierarchy/control-actuator>,
        <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/hierarchy/guidance-processor>,
        <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/hierarchy/sensor-array> ;
    :hasIdentifier [ a :Identifier ;
            schema:name "Missile Guidance System Serial Number" ;
            schema:propertyID "SystemSerial" ;
            schema:value "MGS-2024-001" ] .

<https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/specifications/component-weight-spec> a :QuantityDesignSpecification ;
    rdfs:label "Component Weight Specification"@en ;
    dcterms:source ammo:QuantityDesignSpecification ;
    rdfs:comment "Weight specification for steel component"@en ;
    skos:exactMatch ammo:QuantityDesignSpecification ;
    :maximumValue 2.7 ;
    :minimumValue 2.3 ;
    :nominalValue 2.5 ;
    :toleranceSpec [ a schema:QuantitativeValue ;
            schema:name "Weight tolerance range" ;
            schema:unitCode "KGM" ;
            schema:unitText "kilograms" ;
            schema:value 0.2 ] .

<https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/specifications/steel-316l-material-spec> a :MaterialDesignSpecification ;
    rdfs:label "Steel 316L Material Specification"@en ;
    dcterms:source ammo:MaterialDesignSpecification ;
    rdfs:comment "Material specification for stainless steel 316L used in defense applications"@en ;
    skos:exactMatch ammo:MaterialDesignSpecification ;
    :compositionRequirement "17-20% Chromium, 10-14% Nickel, 2-3% Molybdenum, <0.03% Carbon" ;
    :materialProperty [ a schema:PropertyValue ;
            schema:propertyID "Tensile_Strength" ;
            schema:unitCode "MPA" ;
            schema:unitText "megapascals" ;
            schema:value "515-620" ],
        [ a schema:PropertyValue ;
            schema:propertyID "Corrosion_Resistance" ;
            schema:value "Excellent in marine environments" ] ;
    :qualityStandard "ASTM A240/A240M-20a" ;
    :specifiedMaterial <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/materials/steel-316l-powder> .

<https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/states/in-process> a :ProcessState ;
    rdfs:label "In Process State"@en ;
    schema:description "Material or component is currently undergoing processing or manufacturing" ;
    rdfs:comment "State indicating active manufacturing or processing"@en .

<https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/states/quality-approved> a :LifecycleState ;
    rdfs:label "Quality Approved State"@en ;
    schema:description "Component has passed all quality checks and is approved for use" ;
    rdfs:comment "State indicating successful quality validation"@en .

default1:providesParticipantRole a rdf:Property ;
    rdfs:label "provides participant role"@en ;
    rdfs:comment "Property relating Event to ParticipantRole in this pattern. The range is always ParticipantRole since it makes no sense to have a property named as providesParticipantRole not to point to a participant role. The domain is not set to Event as non-Event may also provide a participant role."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/event> ;
    rdfs:range default1:ParticipantRole .

:Material rdfs:seeAlso ammo:Material ;
    skos:exactMatch ammo:Material ;
    skos:note "Both represent substance classifications that can occur in different amounts with similar characteristics. AMMO Material includes subclasses like Polymer, Alloy, and BaseMaterial."@en .

:hasDirectPart a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:label "has direct part"@en ;
    dcterms:source ammo:hasDirectPart ;
    rdfs:comment "Indicates that one physical object contains another as an immediate part, without any intermediate part-whole relationships."@en ;
    rdfs:domain :PhysicalObject ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:range :PhysicalObject ;
    rdfs:subPropertyOf :hasProperPart ;
    skos:definition "A non-transitive relation indicating direct part containment without intermediates"@en ;
    skos:exactMatch ammo:hasDirectPart ;
    skos:note "Derived from AMMO hasDirectPart for immediate part-whole relationships"@en .

:hasProperPart a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:label "has proper part"@en ;
    dcterms:source ammo:hasProperPart ;
    rdfs:comment "Indicates that one physical object contains another as a proper part. A proper part is a part that is not identical to the whole."@en ;
    rdfs:domain :PhysicalObject ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:range :PhysicalObject ;
    skos:definition "A transitive, antisymmetric, and irreflexive relation indicating proper part containment"@en ;
    skos:exactMatch ammo:hasProperPart ;
    skos:note "Derived from AMMO hasProperPart with formal antisymmetric and irreflexive constraints"@en .

prov:SoftwareAgent rdfs:subClassOf [ a owl:Restriction ;
            rdfs:comment "Software agents may depend on zero or more SPDX packages"@en ;
            owl:allValuesFrom spdx:Package ;
            owl:onProperty default1:dependsOnPackage ] .

default1:ObservationEvent a owl:Class ;
    rdfs:label "Observation Event"@en ;
    rdfs:comment "An event representing the observation or measurement of supply chain items without changing their state, aligned with schema.org observation patterns."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty default1:observationAbout ;
            owl:someValuesFrom owl:Thing ],
        [ a owl:Restriction ;
            owl:onProperty default1:hasObservationResult ;
            owl:someValuesFrom schema:Observation ],
        default1:SupplyChainEvent ;
    skos:definition "An event that records data collection or monitoring activities without altering the observed items, producing structured observations following schema.org standards"@en .

default1:SCDigitalDocument a owl:Class ;
    rdfs:label "Supply Chain Digital Document"@en ;
    rdfs:comment "Represents digital document that is supply chain documentation."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/scdocumentation/document> ;
    rdfs:subClassOf default1:DigitalDocument ;
    skos:definition "A digitized document specifically related to supply chain operations and processes"@en .

default1:TransportEvent a owl:Class ;
    rdfs:label "Transport Event"@en ;
    rdfs:comment "An event representing the movement of goods from one location to another."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty default1:hasOriginLocation ;
            owl:someValuesFrom geo:Feature ],
        [ a owl:Restriction ;
            owl:onProperty default1:hasDestinationLocation ;
            owl:someValuesFrom geo:Feature ],
        default1:SupplyChainEvent ;
    skos:definition "An event that tracks the physical movement of items through the supply chain network"@en .

default1:DocumentContent a owl:Class ;
    rdfs:label "Document Content"@en ;
    rdfs:comment "Represents the content extracted from a document."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/scdocumentation/document> ;
    rdfs:subClassOf prov:Entity ;
    skos:definition "The structured content extracted from a supply chain document through processing workflows"@en .

default1:Identifier rdfs:label "Identifier"@en ;
    rdfs:comment "A generic identifier for entities, aligned with ODP. Represents the abstract identifier value and its type."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/identifier> ;
    rdfs:subClassOf sdo:PropertyValue ;
    skos:definition "A standardized code, number, or reference that uniquely identifies an entity, document, or resource. Holds the core identifier value (via schema:value) and type."@en .

<https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/materials/steel-316l-powder> a :Material ;
    schema:additionalProperty [ a schema:PropertyValue ;
            schema:description "Classified as Alloy in AMMO ontology hierarchy" ;
            schema:propertyID "AMMO_Material_Classification" ;
            schema:value "ammo:Alloy" ],
        [ a schema:PropertyValue ;
            schema:description "Particle size range suitable for powder bed fusion" ;
            schema:propertyID "AM_Powder_Size_Distribution" ;
            schema:unitText "micrometers" ;
            schema:value "15-45 μm" ],
        [ a schema:PropertyValue ;
            schema:description "Chemical composition aligned with ASTM A240 standard" ;
            schema:propertyID "Chemical_Composition_316L" ;
            schema:value "17-20% Cr, 10-14% Ni, 2-3% Mo, <0.03% C" ] ;
    schema:description "Fine stainless steel powder optimized for additive manufacturing processes"@en ;
    schema:name "Stainless Steel 316L Powder"@en ;
    rdfs:seeAlso ammo:BaseMaterial ;
    skos:broader ammo:Alloy ;
    skos:exactMatch ammo:Material .

:QuantityOfMaterial rdfs:seeAlso ammo:Quantity ;
    skos:closeMatch ammo:Material ;
    skos:note "TRU QuantityOfMaterial combines material type with Schema.org QuantitativeValue patterns, while AMMO separates Material classification from Quantity measurement using QUDT standards."@en ;
