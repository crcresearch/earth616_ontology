    rdfs:comment "Relates a transport event to its ending geographic location."@en ;
    rdfs:domain default1:TransportEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:range geo:Feature ;
    rdfs:subPropertyOf geo:hasGeometry .

default1:hasObservationResult a owl:ObjectProperty ;
    rdfs:label "has observation result"@en ;
    rdfs:comment "Links an observation event to the structured observation data produced, typically a schema:Observation instance."@en ;
    rdfs:domain default1:ObservationEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:range schema:Observation ;
    skos:definition "Connects an observation event to its structured result following schema.org observation patterns"@en .

default1:hasOriginLocation a owl:ObjectProperty ;
    rdfs:label "has origin location"@en ;
    rdfs:comment "Relates a transport event to its starting geographic location."@en ;
    rdfs:domain default1:TransportEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:range geo:Feature ;
    rdfs:subPropertyOf geo:hasGeometry .

default1:measurementObservation a owl:ObjectProperty ;
    rdfs:label "measurement observation"@en ;
    rdfs:comment "Links a quantity of material to the schema.org observation that captured its measurement data."@en ;
    rdfs:domain default1:QuantityOfMaterial ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range schema:Observation ;
    skos:definition "Connects material quantities to their observational provenance following schema.org patterns"@en .

<https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru-ammo-alignment> a owl:Ontology ;
    rdfs:label "TRU-AMMO Alignment Module"@en ;
