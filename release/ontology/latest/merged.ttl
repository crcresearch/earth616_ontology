    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:range default1:SupplyChainEvent ;
    rdfs:subPropertyOf time:hasPart .

default1:hasIdentifier a rdf:Property ;
    rdfs:label "has identifier"@en ;
    rdfs:comment "Links an entity to its identifier."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/identifier> ;
    rdfs:range default1:Identifier .

default1:hasIdentifierEncoding a rdf:Property ;
    rdfs:label "has identifier encoding"@en ;
    rdfs:comment "Links an identifier to its physical or digital encoding."@en ;
    rdfs:domain default1:Identifier ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/identifier> ;
    rdfs:range default1:IdentifierEncoding .

default1:hasIdentifierSource a rdf:Property ;
    rdfs:label "has identifier source"@en ;
    rdfs:comment "Links an identifier to its issuing authority or source."@en ;
    rdfs:domain default1:Identifier ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/identifier> ;
    rdfs:range default1:IdentifierSource .

default1:hasIdentifierType a rdf:Property ;
    rdfs:label "has identifier type"@en ;
    rdfs:comment "Links an identifier to its type classification."@en ;
    rdfs:domain default1:Identifier ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/identifier> ;
    rdfs:range default1:IdentifierType .

default1:hasInformationObject a rdf:Property ;
    rdfs:label "has information object"@en ;
    rdfs:comment "Property relating Event to InformationObject in this pattern. The domain of this property is not restricted to Event since it is possible that non-event to have an information object. The range is always InformationObject since it makes no sense to have a property named hasInformationObject to point to anything other than information object."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/event> ;
    rdfs:range default1:InformationObject .

default1:hasMeasurementMethod a owl:ObjectProperty ;
    rdfs:label "has measurement method"@en ;
    rdfs:comment "Specifies how the quantity measurement was obtained"@en ;
    rdfs:domain default1:QuantityOfMaterial ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:subPropertyOf schema:measurementMethod .

default1:hasShelfLife a owl:DatatypeProperty ;
    rdfs:label "has shelf life"@en ;
    rdfs:comment "Expected shelf life or expiration duration"@en ;
    rdfs:domain default1:TraceableResourceUnit ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range xsd:duration ;
    skos:definition "The duration for which the TRU remains suitable for use"@en .

default1:hasWorkflow a owl:ObjectProperty ;
    rdfs:label "has workflow"@en ;
    rdfs:comment "Associates an information extraction workflow with a specific pipeline."@en ;
    rdfs:domain default1:InformationExtractionWorkflow ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/scdocumentation/document> ;
    rdfs:range default1:IEPipeline ;
    rdfs:subPropertyOf prov:wasAssociatedWith .

default1:inputTRU a owl:ObjectProperty ;
    rdfs:label "input TRU"@en ;
    rdfs:comment "TRU that serves as input to a transformation event"@en ;
    rdfs:domain default1:TransformationEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range default1:TraceableResourceUnit ;
    skos:example """
{
  "@type": "TransformationEvent",
  "@id": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/events/steel-cutting-001",
  "startedAtTime": "2024-03-15T09:00:00Z",
  "endedAtTime": "2024-03-15T11:30:00Z",
  "inputTRU": {
    "@type": "TraceableResourceUnit",
    "hasIdentifier": {"value": "SB-2024-001", "propertyID": "BatchNumber"},
    "consistsOf": {"value": 25.5, "unitCode": "KGM"}
  },
  "outputTRU": [
    {
      "@type": "TraceableResourceUnit", 
      "hasIdentifier": {"value": "SC-001-2024", "propertyID": "ComponentSerial"},
      "consistsOf": {"value": 12.2, "unitCode": "KGM"},
      "predecessorOf": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/tru/steel-batch-001"
    },
    {
      "@type": "TraceableResourceUnit",
      "hasIdentifier": {"value": "SC-002-2024", "propertyID": "ComponentSerial"}, 
      "consistsOf": {"value": 13.3, "unitCode": "KGM"},
      "predecessorOf": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/tru/steel-batch-001"
    }
  ]
}
"""@en .

default1:involvesTRU a owl:ObjectProperty ;
    rdfs:label "involves TRU"@en ;
    rdfs:comment "Links a supply chain event to the TRU roles it involves"@en ;
    rdfs:domain default1:SupplyChainEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range default1:TRURole ;
    rdfs:subPropertyOf default1:providesParticipantRole ;
    skos:definition "Connects supply chain events to the TRU roles that participate in them"@en .

default1:measuredProperty a owl:ObjectProperty ;
    rdfs:label "measured property"@en ;
    rdfs:comment "The property or characteristic being measured in the observation, aligned with schema.org measuredProperty."@en,
        "The specific property of the material being quantified"@en ;
    rdfs:domain schema:Observation,
        default1:QuantityOfMaterial ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule>,
        <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range owl:Thing ;
    rdfs:subPropertyOf schema:measuredProperty ;
    skos:definition "Specifies what characteristic is being measured (e.g., temperature, weight, location)"@en .

default1:measurementValue a owl:ObjectProperty ;
    rdfs:label "measurement value"@en ;
    rdfs:comment "The quantitative or qualitative value resulting from the observation, typically a QuantitativeValue."@en ;
    rdfs:domain schema:Observation ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:range [ owl:unionOf ( schema:QuantitativeValue schema:PropertyValue xsd:string xsd:boolean ) ] ;
    rdfs:subPropertyOf schema:value ;
    skos:definition "The measured value or result of the observation activity"@en .

<https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/spdx/sbom-integration> a owl:Ontology ;
    rdfs:label "DSCDO SPDX SBOM Integration Module"@en ;
    dcterms:created "2025-01-25"^^xsd:date ;
    dcterms:creator <https://orcid.org/0000-0003-4091-6059> ;
    dcterms:modified "2025-01-25"^^xsd:date ;
    rdfs:comment "Integrates SPDX 3.0.1 Software Bill of Materials (SBOM) concepts with DSCDO workflow ontology for container-based AI agent provenance tracking."@en ;
    rdfs:seeAlso <https://spdx.github.io/spdx-spec/v3.0.1/> ;
    owl:imports <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/event>,
        spdx: ;
    owl:versionInfo "0.1.0" .

default1:observationDate a owl:DatatypeProperty ;
    rdfs:label "observation date"@en ;
    rdfs:comment "The date/time when the observation was made, aligned with schema.org observationDate."@en ;
    rdfs:domain default1:ObservationEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:range xsd:dateTime ;
    rdfs:subPropertyOf schema:observationDate ;
    skos:definition "Timestamp indicating when the observation was recorded"@en .

default1:observationIntegration skos:example """
{
  "@context": {
    "@vocab": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/",
    "schema": "http://schema.org/",
    "Observation": "schema:Observation",
    "observationAbout": "schema:observationAbout",
    "observationDate": "schema:observationDate",
    "measuredProperty": "schema:measuredProperty",
    "value": "schema:value"
  },
  "@graph": [
    {
      "@type": "ObservationEvent",
      "@id": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/events/mass-measurement-001",
      "startedAtTime": "2024-03-15T08:45:00Z",
      "observationAbout": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/tru/steel-batch-001",
      "observationDate": "2024-03-15T08:45:00Z",
      "hasObservationResult": {
        "@type": "Observation",
        "@id": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/observations/mass-obs-001",
        "observationAbout": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/tru/steel-batch-001",
        "observationDate": "2024-03-15T08:45:00Z",
        "measuredProperty": "mass",
        "value": {
          "@type": "QuantityOfMaterial",
          "value": 25.5,
          "unitCode": "KGM",
          "unitText": "kilograms",
          "hasMeasurementMethod": "industrial_scale_weighing",
          "measurementObservation": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/observations/mass-obs-001"
        },
        "observationMethod": "digital_industrial_scale",
        "marginOfError": 0.1
      }
    }
  ]
}
"""@en .

default1:observationMethod a rdf:Property ;
    rdfs:label "observation method"@en ;
    rdfs:comment "Recommended to use Wikidata URIs for standardized measurement techniques (e.g., Q134566 for weighing scale, Q1402042 for precision balance, Q180809 for mass spectrometry)"@en,
        "The technique or method used to make the observation, extending schema.org measurementTechnique. Can be a string (e.g., 'digital_scale') or a structured object describing the method."@en ;
    rdfs:domain schema:Observation ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:subPropertyOf schema:measurementTechnique ;
    skos:definition "Describes how the observation was performed (sensor type, inspection method, etc.)"@en ;
    skos:example "\"digital_industrial_scale\""@en,
        "<http://www.wikidata.org/entity/Q134566>"@en,
        "<http://www.wikidata.org/entity/Q1402042>"@en .

default1:observedQuantity a owl:ObjectProperty ;
    rdfs:label "observed quantity"@en ;
    rdfs:comment "Links a schema.org observation to the quantity of material that was measured."@en ;
    rdfs:domain schema:Observation ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range default1:QuantityOfMaterial ;
    owl:inverseOf default1:measurementObservation ;
    skos:definition "Inverse relationship connecting observations to their material quantity results"@en .

default1:ofMaterialType a owl:ObjectProperty ;
    rdfs:label "of material type"@en ;
    rdfs:comment "Links a quantity of material to its material type"@en ;
    rdfs:domain default1:QuantityOfMaterial ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range default1:Material ;
    skos:definition "Specifies the type or classification of material in a quantity"@en .

default1:outputTRU a owl:ObjectProperty ;
    rdfs:label "output TRU"@en ;
    rdfs:comment "TRU that results from a transformation event"@en ;
    rdfs:domain default1:TransformationEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range default1:TraceableResourceUnit .

default1:partOf a owl:ObjectProperty ;
    rdfs:label "part of"@en ;
    rdfs:comment "Hierarchical relationship between TRUs"@en ;
    rdfs:domain default1:TraceableResourceUnit ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range default1:TraceableResourceUnit ;
    skos:definition "Indicates that one TRU is a component or part of another TRU"@en .

default1:performsTRURole a owl:ObjectProperty ;
    rdfs:label "performs TRU role"@en ;
    rdfs:comment "Links a TRU to the role it performs in an event"@en ;
    rdfs:domain default1:TraceableResourceUnit ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range default1:TRURole ;
    skos:definition "Identifies which role a traceable resource unit performs in an event"@en .

default1:precedesEvent a owl:ObjectProperty ;
    rdfs:label "precedes event"@en ;
    rdfs:comment "Indicates temporal precedence between supply chain events."@en ;
    rdfs:domain default1:SupplyChainEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/supplychain/eventmodule> ;
    rdfs:range default1:SupplyChainEvent ;
    rdfs:subPropertyOf time:before .

default1:predecessorOf a owl:ObjectProperty ;
    rdfs:label "predecessor of"@en ;
    rdfs:comment "Links TRU to its predecessor before transformation"@en ;
    rdfs:domain default1:TraceableResourceUnit ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range default1:TraceableResourceUnit ;
    skos:definition "Indicates the transformation lineage between TRUs across events"@en .

default1:processedText a owl:DatatypeProperty ;
    rdfs:label "processed text"@en ;
    rdfs:comment "The processed text of a digitized document."@en ;
    rdfs:domain default1:DocumentContent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/scdocumentation/document> ;
    rdfs:range xsd:string .

default1:sbomGeneratedBy a owl:ObjectProperty ;
    rdfs:label "SBOM generated by"@en ;
    rdfs:comment "Links an SBOM to the tool or agent that generated it, extending SPDX creation info with DSCDO agent concepts."@en ;
    rdfs:domain spdx:Sbom ;
    rdfs:range [ owl:unionOf ( prov:SoftwareAgent default1:SoftwareAgent ) ] ;
    rdfs:subPropertyOf prov:wasGeneratedBy ;
    skos:definition "Tracks which scanning tools, container runtimes, or AI agents were responsible for generating SBOM data."@en .

default1:subEventOf a rdf:Property ;
    rdfs:label "sub event of"@en ;
    rdfs:comment "Property expressing partonomic relation between two events. Domain and range are always Event."@en ;
    rdfs:domain default1:Event ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/event> ;
    rdfs:range default1:Event .

default1:transferredTRU a owl:ObjectProperty ;
    rdfs:label "transferred TRU"@en ;
    rdfs:comment "TRU that is transferred in a transfer event"@en ;
    rdfs:domain default1:TransferEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:range default1:TraceableResourceUnit .

default1:workflowExecution a owl:ObjectProperty ;
    rdfs:label "workflow execution"@en ;
    rdfs:comment "Connects the document content to the workflow that processed it."@en ;
    rdfs:domain default1:DocumentContent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/scdocumentation/document> ;
    rdfs:range default1:InformationExtractionWorkflow ;
    rdfs:subPropertyOf prov:wasGeneratedBy .

:acceptanceCriteria a owl:DatatypeProperty ;
    rdfs:label "acceptance criteria"@en ;
    rdfs:comment "Defines the specific criteria that must be met for acceptance or approval."@en ;
    rdfs:domain :QualitySpecification ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    rdfs:range xsd:string ;
    skos:definition "Textual description of acceptance requirements"@en ;
    skos:example "Surface roughness < 1.6 μm, No visible defects, Dimensional accuracy ± 0.1mm"@en .

:assembles a owl:ObjectProperty ;
    rdfs:label "assembles"@en ;
    rdfs:comment "Indicates that an assembly event combines specific components."@en ;
    rdfs:domain :AssemblyEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:range :Component ;
    skos:definition "Links assembly events to the components being assembled"@en .

:compositionRequirement a owl:DatatypeProperty ;
    rdfs:label "composition requirement"@en ;
    rdfs:comment "Specifies the required chemical or physical composition for a material."@en ;
    rdfs:domain :MaterialDesignSpecification ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    rdfs:range xsd:string ;
    skos:definition "Textual specification of required material composition"@en ;
    skos:example "18-20% Chromium, 8-10.5% Nickel, <0.08% Carbon"@en .

:conformsToSpecification a owl:ObjectProperty ;
    rdfs:label "conforms to specification"@en ;
    rdfs:comment "Indicates that a traceable resource unit meets the requirements defined in a design specification."@en ;
    rdfs:domain :TraceableResourceUnit ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:range :DesignSpecification ;
    skos:definition "Links TRUs to the specifications they satisfy"@en ;
    skos:example "Manufactured component conforms to military specification MIL-STD-810"@en .

:consistsOf rdfs:seeAlso ammo:ofQuantity ;
    skos:note "Both properties relate objects to their quantified composition. AMMO uses QUDT-based quantities while TRU uses Schema.org QuantitativeValue patterns."@en ;
    skos:relatedMatch ammo:ofQuantity .

:disassembles a owl:ObjectProperty ;
    rdfs:label "disassembles"@en ;
    rdfs:comment "Indicates that a disassembly event breaks down a specific system."@en ;
    rdfs:domain :DisassemblyEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:range :DesignedPhysicalSystem ;
    skos:definition "Links disassembly events to the systems being disassembled"@en .

:executesDesign a owl:ObjectProperty ;
    rdfs:label "executes design"@en ;
    rdfs:comment "Indicates that a supply chain event follows or implements a specific design or process plan."@en ;
    rdfs:domain :SupplyChainEvent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:range :Design ;
    skos:definition "Links events to the designs or process plans they execute"@en ;
    skos:example "Manufacturing event executes assembly design plan"@en .

:hasFunction a owl:DatatypeProperty ;
    rdfs:label "has function"@en ;
    rdfs:comment "Describes the primary function or purpose of a component within a system."@en ;
    rdfs:domain :Component ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:range xsd:string ;
    skos:definition "Textual description of component functionality"@en ;
    skos:example "structural support, power conversion, signal processing, environmental protection"@en .

:hasTransition a owl:ObjectProperty ;
    rdfs:label "has transition"@en ;
    dcterms:source ammo:hasTransition ;
    rdfs:comment "Indicates that an entity undergoes a specific state transition."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    rdfs:range :Transition ;
    skos:definition "Links entities to the transitions they undergo"@en ;
    skos:exactMatch ammo:hasTransition ;
    skos:note "Derived from AMMO hasTransition property"@en .

:hasVersion a owl:ObjectProperty ;
    rdfs:label "has version"@en ;
    rdfs:comment "Links a design to its specific versions or revisions."@en ;
    rdfs:domain :Design ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:range :DesignVersion ;
    skos:definition "Connects designs to their versioned instances"@en .

:implementsDesign a owl:ObjectProperty ;
    rdfs:label "implements design"@en ;
    rdfs:comment "Indicates that a traceable resource unit is manufactured or assembled according to a specific design."@en ;
    rdfs:domain :TraceableResourceUnit ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:range :Design ;
    skos:definition "Links TRUs to the designs they implement or conform to"@en ;
    skos:example "Steel component TRU implements Design XYZ-2024-001"@en .

:isComponentOf a owl:ObjectProperty ;
    rdfs:label "is component of"@en ;
    dcterms:source ammo:isComponentOf ;
    rdfs:comment "Indicates that this designed component is a functional part of a designed physical system."@en ;
    rdfs:domain :DesignedComponent ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:range :DesignedPhysicalSystem ;
    rdfs:subPropertyOf :isDirectPartOf ;
    owl:inverseOf :hasComponent ;
    skos:definition "Inverse relationship linking components to their containing systems"@en ;
    skos:exactMatch ammo:isComponentOf ;
    skos:note "Derived from AMMO isComponentOf inverse relationship"@en .

:isDescribedBy a owl:ObjectProperty ;
    rdfs:label "is described by"@en ;
    dcterms:source ammo:isDescribedBy ;
    rdfs:comment "Indicates that an entity is specified or blueprinted by a particular design."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:range :Design ;
    owl:inverseOf :describes ;
    skos:definition "Links entities back to the designs that specify them"@en ;
    skos:exactMatch ammo:isDescribedBy ;
    skos:note "Derived from AMMO isDescribedBy inverse property"@en .

:isDesignSpecificationOf a owl:ObjectProperty ;
    rdfs:label "is design specification of"@en ;
    rdfs:comment "Indicates that this specification defines requirements for a particular design."@en ;
    rdfs:domain :DesignSpecification ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:range :Design ;
    owl:inverseOf :hasDesignSpecification ;
    skos:definition "Inverse property linking specifications back to the designs they define"@en .

:isSpecifiedBy a owl:ObjectProperty ;
    rdfs:label "is specified by"@en ;
    rdfs:comment "Indicates that an entity is defined or constrained by a particular design specification."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:range :DesignSpecification ;
    owl:inverseOf :specifies ;
    skos:definition "Links entities back to the specifications that define their requirements"@en .

:materialProperty a owl:ObjectProperty ;
    rdfs:label "material property"@en ;
    rdfs:comment "Links a material specification to specific properties that must be satisfied."@en ;
    rdfs:domain :MaterialDesignSpecification ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    skos:definition "Connects specifications to the material properties they define"@en .

:maximumValue a owl:DatatypeProperty ;
    rdfs:label "maximum value"@en ;
    rdfs:comment "The maximum acceptable value for a quantity measurement."@en ;
    rdfs:domain :QuantityDesignSpecification ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    rdfs:range xsd:decimal ;
    skos:definition "Upper bound for acceptable quantity values"@en .

:meetsSpecification a owl:ObjectProperty ;
    rdfs:label "meets specification"@en ;
    rdfs:comment "Indicates that a material quantity satisfies the requirements defined in a material design specification."@en ;
    rdfs:domain :SpecifiedQuantityOfMaterial ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    rdfs:range :MaterialDesignSpecification ;
    skos:definition "Links material quantities to the specifications they satisfy"@en .

:minimumValue a owl:DatatypeProperty ;
    rdfs:label "minimum value"@en ;
    rdfs:comment "The minimum acceptable value for a quantity measurement."@en ;
