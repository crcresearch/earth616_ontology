@prefix : <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont> .
@prefix ammo: <https://w3id.org/ammo/ont/> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix default1: <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix gs11: <https://gs1.org/voc/> .
@prefix lode: <http://purl.org/lode/config#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prof: <http://www.w3.org/ns/dx/prof/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sdo: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix spdx: <https://spdx.org/rdf/3.0.1/terms/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

default1:Agent a rdfs:Class ;
    rdfs:label "Agent"@en ;
    rdfs:comment "An entity participating in an event, such as a person, organization, or system."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/event> ;
    skos:definition "An entity capable of acting or participating in events"@en .

default1:Event a rdfs:Class ;
    rdfs:label "Event"@en ;
    rdfs:comment "Represents any kind of events. An event possesses a spatiotemporal extent, provides at least one participant-role, which is performed by some entity (agents or otherwise), and may be a sub-event of another event. An event may also have additional descriptive information, which is consolidated through an information object."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/event> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty default1:hasSpatioTemporalExtent ;
            owl:someValuesFrom default1:SpatioTemporalExtent ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty default1:providesParticipantRole ] ;
    skos:definition "A temporal entity that represents occurrences or happenings in time and space"@en .

default1:IdentifierEncoding a rdfs:Class ;
    rdfs:label "Identifier Encoding"@en ;
    rdfs:comment "Represents a specific instance of how an identifier is encoded (e.g., a specific barcode instance, an RFID tag encoding)."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/identifier> ;
    skos:definition "A physical or digital encoding of an identifier, such as a barcode, QR code, or RFID tag"@en .

default1:IdentifierSource a rdfs:Class ;
    rdfs:label "Identifier Source"@en ;
    rdfs:comment "The authority or organization that issued or manages the identifier."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/identifier> ;
    skos:definition "An organization or system that issues, maintains, or governs an identifier scheme"@en .

default1:IdentifierType a rdfs:Class ;
    rdfs:label "Identifier Type"@en ;
    rdfs:comment "Represents the type or category of an identifier (e.g., GTIN, ISBN, DOI)."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/identifier> ;
    skos:definition "A classification of identifier types, specifying the standard or system used"@en .

default1:InformationObject a rdfs:Class ;
    rdfs:label "Information Object"@en ;
    rdfs:comment "An entity that encapsulates all descriptive or non-defining information of the corresponding event, e.g., names, additional identifiers, textual descriptions, etc. Should normally be aligned to a separate Information Object pattern."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/event> ;
    skos:definition "An object that carries descriptive information about an event"@en .

default1:Material a rdfs:Class ;
    rdfs:label "Material"@en ;
    rdfs:comment "The type or classification of material within a quantity of material."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    skos:definition "A classification of substance or material used in supply chain operations"@en ;
    skos:exactMatch ammo:Material ;
    skos:example "Stainless Steel Grade 304, Aluminum Alloy 6061, Polymer ABS plastic"@en ;
    skos:note "Aligns with AMMO Material concept: 'Substance that can occur in different amounts, all with some similar characteristics'. AMMO provides subclasses like Alloy, Polymer, and BaseMaterial."@en .

default1:ParticipantRole a rdfs:Class ;
    rdfs:label "Participant Role"@en ;
    rdfs:comment "Every ParticipantRole represents a reified relationship between an event and one of its participants. Should normally be aligned to a separate Participant-Role pattern."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/core/event> ;
    skos:definition "A role played by an agent or entity participating in an event"@en .

default1:QuantityOfMaterial a rdfs:Class ;
    rdfs:label "Quantity Of Material"@en ;
    rdfs:comment "Represents a specific quantity of material with defined properties and measurements. Inherits from schema:QuantitativeValue to support standard quantity expressions with value, unitCode, and measurement properties."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:subClassOf schema:QuantitativeValue,
        prov:Entity ;
    skos:definition "A measurable amount of material that can be part of a traceable resource unit, expressed using Schema.org quantity patterns"@en ;
    skos:example "25.5 kilograms of steel measured using industrial scale weighing, with unitCode 'KGM' per UN/CEFACT standards"@en .

default1:SourceTRURole a rdfs:Class ;
    rdfs:label "Source TRU Role"@en ;
    rdfs:comment "Role of TRU as input/source in a transformation event"@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:subClassOf default1:TRURole ;
    skos:definition "The role of a TRU serving as input or source material in an event"@en .

default1:TRURole a rdfs:Class ;
    rdfs:label "TRU Role"@en ;
    rdfs:comment "Role played by a TRU in an event"@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:subClassOf default1:ParticipantRole ;
    skos:definition "A role performed by a traceable resource unit in a supply chain event"@en .

default1:TargetTRURole a rdfs:Class ;
    rdfs:label "Target TRU Role"@en ;
    rdfs:comment "Role of TRU as output/target in a transformation event"@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:subClassOf default1:TRURole ;
    skos:definition "The role of a TRU serving as output or target result in an event"@en .

default1:TraceableResourceUnit a rdfs:Class ;
    rdfs:label "Traceable Resource Unit"@en ;
    rdfs:comment "A homogeneous collection of resources categorized by a class and quantified by its interaction with a primitive activity. A uniquely identifiable unit that carries essential information about a product as it moves through the supply chain."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/traceability/tru> ;
    rdfs:subClassOf prov:Entity,
        gs11:IndividualObject ;
    skos:closeMatch ammo:DesignedArtifact ;
    skos:definition "A fundamental traceability unit defined by batch number, trade unit, or logistics unit, used to track transformations like packaging or splitting."@en ;
    skos:example """
{
  "@type": "TraceableResourceUnit",
  "@id": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/examples/tru/steel-batch-001",
  "hasIdentifier": {
    "@type": "Identifier",
    "value": "SB-2024-001",
    "propertyID": "BatchNumber",
    "name": "Steel Production Batch"
  },
  "hasBatchNumber": "SB-2024-001",
  "hasShelfLife": "P2Y",
  "consistsOf": {
    "@type": "QuantityOfMaterial",
    "value": 25.5,
    "unitCode": "KGM",
    "unitText": "kilograms",
    "ofMaterialType": {
      "@type": "Material",
      "name": "Stainless Steel Grade 304"
    },
    "measuredProperty": "mass",
    "hasMeasurementMethod": "industrial_scale_weighing"
  }
}
"""@en,
        "A batch of steel alloy with batch number 'SB-2024-001' containing 25.5 kg of Grade 304 stainless steel"@en ;
    skos:note "Aligns with AMMO DesignedArtifact concept for traceable physical objects. AMMO emphasizes design specifications and manufacturing provenance, while TRU emphasizes supply chain traceability with batch/lot tracking."@en .

:AssemblyEvent a rdfs:Class ;
    rdfs:label "Assembly Event"@en ;
    rdfs:comment "An event that represents the assembly of multiple components into a larger system or sub-assembly."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:subClassOf :SupplyChainEvent ;
    skos:definition "An event tracking the composition of components into larger assemblies"@en ;
    skos:example "Engine assembly process, circuit board population, missile integration"@en .

:Component a rdfs:Class ;
    rdfs:label "Component"@en ;
    dcterms:source ammo:Component ;
    rdfs:comment "A physical object that serves a specific function within a larger system or assembly."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:subClassOf :PhysicalObject ;
    skos:definition "A functional part of a larger system, designed to perform a specific role"@en ;
    skos:exactMatch ammo:Component ;
    skos:example "Engine component, circuit board, structural element, sensor module"@en ;
    skos:note "Derived from AMMO Component class for functional parts of systems"@en .

:ComponentDesign a rdfs:Class ;
    rdfs:label "Component Design"@en ;
    dcterms:source ammo:Design ;
    rdfs:comment "A design that specifies the structure, properties, and behavior of an individual component within a larger system."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:subClassOf :Design ;
    skos:closeMatch ammo:Design ;
    skos:definition "A design focused on a specific component, including its interfaces, requirements, and implementation details"@en ;
    skos:example "Design for a steel bracket component, specification for an electronic module, blueprint for a mechanical assembly"@en ;
    skos:note "Specialized from AMMO Design class for component-level design specifications"@en .

:ComponentRole a rdfs:Class ;
    rdfs:label "Component Role"@en ;
    rdfs:comment "A role played by a component within a system, defining its function and responsibilities."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:subClassOf :ParticipantRole ;
    skos:definition "A functional role that defines how a component contributes to system operation"@en ;
    skos:example "Primary structural role, backup power role, sensor input role, control output role"@en .

:ComponentTRU a rdfs:Class ;
    rdfs:label "Component TRU"@en ;
    rdfs:comment "A traceable resource unit that functions as a component within a larger system, combining supply chain traceability with hierarchical relationships."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:subClassOf :Component,
        :TraceableResourceUnit ;
    skos:definition "A TRU that participates in component hierarchies while maintaining traceability"@en ;
    skos:example "Traceable steel bracket component, serialized electronic module, identified mechanical part"@en .

:Design a rdfs:Class ;
    rdfs:label "Design"@en ;
    dcterms:source ammo:Design ;
    rdfs:comment "A plan or specification for the construction of an object, system, or process. Represents the abstract blueprint that guides physical implementation."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    skos:definition "An abstract plan or blueprint that specifies how to construct, assemble, or implement a physical or conceptual entity"@en ;
    skos:exactMatch ammo:Design ;
    skos:example "Engineering drawings for a defense component, software architecture for a supply chain system, assembly instructions for a traceable resource unit"@en ;
    skos:note "Derived from AMMO Design class: 'A plan or specification for the construction of an object or system'"@en .

:DesignSpecification a rdfs:Class ;
    rdfs:label "Design Specification"@en ;
    dcterms:source ammo:DesignSpecification ;
    rdfs:comment "A detailed document that sets out exactly what a product, system, or process should present. Contains specific requirements, constraints, and implementation details."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:subClassOf :InformationObject ;
    skos:definition "A formal document containing detailed requirements and constraints that must be satisfied by a design implementation"@en ;
    skos:exactMatch ammo:DesignSpecification ;
    skos:example "Technical specification document for steel alloy composition, CAD file with dimensional tolerances, quality control checklist for manufacturing process"@en ;
    skos:note "Derived from AMMO DesignSpecification: 'detailed document that sets out exactly what a product or process should present'"@en .

:DesignVersion a rdfs:Class ;
    rdfs:label "Design Version"@en ;
    rdfs:comment "A specific version or revision of a design, enabling tracking of design evolution and change management."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    skos:definition "A timestamped version of a design that supports version control and change tracking"@en ;
    skos:example "Version 2.1 of missile component design, Revision C of manufacturing process specification"@en ;
    skos:note "Extension of AMMO design patterns to support version control and design evolution tracking"@en .

:DesignedComponent a rdfs:Class ;
    rdfs:label "Designed Component"@en ;
    dcterms:source ammo:DesignedComponent ;
    rdfs:comment "A component that has been explicitly designed according to specifications and requirements."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:subClassOf :Component ;
    skos:definition "A component with explicit design specifications and functional requirements"@en ;
    skos:exactMatch ammo:DesignedComponent ;
    skos:example "Custom-machined part, engineered subsystem, specified material component"@en ;
    skos:note "Derived from AMMO DesignedComponent for components with explicit design specifications"@en .

:DesignedPhysicalSystem a rdfs:Class ;
    rdfs:label "Designed Physical System"@en ;
    dcterms:source ammo:DesignedPhysicalSystem ;
    rdfs:comment "A complex physical object composed of multiple components that work together to achieve system-level functionality."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:subClassOf :PhysicalObject ;
    skos:definition "A system composed of designed components working together to fulfill system requirements"@en ;
    skos:exactMatch ammo:DesignedPhysicalSystem ;
    skos:example "Missile guidance system, manufacturing assembly line, integrated defense platform"@en ;
    skos:note "Derived from AMMO DesignedPhysicalSystem for complex component assemblies"@en .

:DisassemblyEvent a rdfs:Class ;
    rdfs:label "Disassembly Event"@en ;
    rdfs:comment "An event that represents the disassembly of a system into its component parts."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:subClassOf :SupplyChainEvent ;
    skos:definition "An event tracking the decomposition of assemblies into individual components"@en ;
    skos:example "Maintenance disassembly, component recycling, failure analysis"@en .

:LifecycleState a rdfs:Class ;
    rdfs:label "Lifecycle State"@en ;
    rdfs:comment "A state that represents a phase in the lifecycle of a traceable resource unit or component."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    rdfs:subClassOf :State ;
    skos:definition "A state representing lifecycle phases for supply chain entities"@en ;
    skos:example "Designed, Manufactured, Tested, Delivered, Deployed, Maintained, Retired"@en .

:MaterialDesignSpecification a rdfs:Class ;
    rdfs:label "Material Design Specification"@en ;
    dcterms:source ammo:MaterialDesignSpecification ;
    rdfs:comment "A design specification that defines requirements and constraints for material properties, composition, and behavior."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    rdfs:subClassOf :DesignSpecification ;
    skos:definition "A formal specification document that details material requirements including composition, properties, and performance criteria"@en ;
    skos:exactMatch ammo:MaterialDesignSpecification ;
    skos:example "Steel alloy composition specification, polymer property requirements, ceramic performance criteria"@en ;
    skos:note "Derived from AMMO MaterialDesignSpecification for material requirement definition"@en .

:MaterialState a rdfs:Class ;
    rdfs:label "Material State"@en ;
    rdfs:comment "A state that represents the condition or phase of a material or substance."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    rdfs:subClassOf :State ;
    skos:definition "A state describing material conditions and phases"@en ;
    skos:example "Solid state, Liquid state, Powder state, Molten state, Cured state"@en .

:PhysicalObject a rdfs:Class ;
    rdfs:label "Physical Object"@en ;
    dcterms:source ammo:PhysicalObject ;
    rdfs:comment "A concrete, spatially extended entity that can exist as a whole or as a part of other physical objects."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    skos:definition "A material entity with spatial extent that can participate in part-whole relationships"@en ;
    skos:exactMatch ammo:PhysicalObject ;
    skos:example "Steel component, electronic module, mechanical assembly, complete missile system"@en ;
    skos:note "Derived from AMMO PhysicalObject: base class for spatial entities in part-whole hierarchies"@en .

:ProcessDesignSpecification a rdfs:Class ;
    rdfs:label "Process Design Specification"@en ;
    rdfs:comment "A design specification that defines requirements and constraints for manufacturing processes, procedures, and workflows."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    rdfs:subClassOf :DesignSpecification ;
    skos:definition "A formal specification document that details process requirements including parameters, sequences, and quality criteria"@en ;
    skos:example "Manufacturing process parameters, assembly procedures, quality control workflows"@en .

:ProcessState a rdfs:Class ;
    rdfs:label "Process State"@en ;
    rdfs:comment "A state that represents a particular phase or condition within a process or workflow."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    rdfs:subClassOf :State ;
    skos:definition "A state specific to process phases and workflow conditions"@en ;
    skos:example "Manufacturing state, Quality control state, Assembly state, Testing state"@en .

:QualitySpecification a rdfs:Class ;
    rdfs:label "Quality Specification"@en ;
    rdfs:comment "A design specification that defines quality requirements, testing procedures, and acceptance criteria."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    rdfs:subClassOf :DesignSpecification ;
    skos:definition "A formal specification document that details quality requirements and validation procedures"@en ;
    skos:example "Military standard compliance, testing protocols, acceptance criteria"@en .

:QuantityDesignSpecification a rdfs:Class ;
    rdfs:label "Quantity Design Specification"@en ;
    dcterms:source ammo:QuantityDesignSpecification ;
    rdfs:comment "A design specification that defines requirements and constraints for quantities, measurements, and dimensional properties."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    rdfs:subClassOf :DesignSpecification ;
    skos:definition "A formal specification document that details quantity requirements including dimensions, tolerances, and measurement criteria"@en ;
    skos:exactMatch ammo:QuantityDesignSpecification ;
    skos:example "Dimensional tolerance specification, weight requirements, volume constraints"@en ;
    skos:note "Derived from AMMO QuantityDesignSpecification for quantity requirement definition"@en .

:SpecificationValidationEvent a rdfs:Class ;
    rdfs:label "Specification Validation Event"@en ;
    rdfs:comment "An event that represents the validation or testing of materials, components, or processes against design specifications."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    rdfs:subClassOf :SupplyChainEvent ;
    skos:definition "An event tracking compliance verification against formal specifications"@en ;
    skos:example "Material composition testing, dimensional inspection, quality assurance validation"@en .

:SpecifiedQuantityOfMaterial a rdfs:Class ;
    rdfs:label "Specified Quantity Of Material"@en ;
    rdfs:comment "A quantity of material that has been measured and validated against formal design specifications."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/specification> ;
    rdfs:subClassOf :QuantityOfMaterial ;
    skos:definition "A material quantity with formal specification compliance"@en ;
    skos:example "Steel quantity verified against MIL-STD composition requirements"@en .

:State a rdfs:Class ;
    rdfs:label "State"@en ;
    dcterms:source ammo:State ;
    rdfs:comment "A condition or mode of being that an entity can exist in at a particular point in time. States are discrete and mutually exclusive."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    skos:definition "A discrete condition or configuration that characterizes an entity at a specific time"@en ;
    skos:exactMatch ammo:State ;
    skos:example "Raw material state, In-process state, Completed state, Shipped state, Installed state"@en ;
    skos:note "Derived from AMMO State class for entity condition modeling"@en .

:StateOccurrence a rdfs:Class ;
    rdfs:label "State Occurrence"@en ;
    rdfs:comment "A specific instance of an entity being in a particular state during a defined time interval."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    skos:definition "A timestamped record of an entity being in a specific state"@en ;
    skos:example "Component was in 'Testing' state from 2024-03-15 to 2024-03-16"@en .

:StateTransitionEvent a rdfs:Class ;
    rdfs:label "State Transition Event"@en ;
    rdfs:comment "A supply chain event that represents a state transition for one or more entities."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    rdfs:subClassOf :SupplyChainEvent ;
    skos:definition "An event that triggers or records state changes"@en ;
    skos:example "Quality approval event, Manufacturing completion event, Shipping initiation event"@en .

:StatefulTRU a rdfs:Class ;
    rdfs:label "Stateful TRU"@en ;
    rdfs:comment "A traceable resource unit with explicit state tracking throughout its lifecycle."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
    rdfs:subClassOf :TraceableResourceUnit ;
    skos:definition "A TRU with formal state management and transition tracking"@en ;
    skos:example "Steel component TRU with states: Raw → Machined → Tested → Approved → Shipped"@en .

:SystemDesign a rdfs:Class ;
    rdfs:label "System Design"@en ;
    dcterms:source ammo:SystemDesign ;
    rdfs:comment "A design that describes the architecture, components, modules, interfaces, and data flow for a system to satisfy specified requirements."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/composition> ;
    rdfs:subClassOf :Design ;
    skos:definition "A comprehensive design that specifies system structure, component relationships, and functional requirements"@en ;
    skos:exactMatch ammo:SystemDesign ;
    skos:example "Defense system architecture, supply chain workflow design, integrated manufacturing system blueprint"@en ;
    skos:note "Derived from AMMO SystemDesign class for comprehensive system-level design specifications"@en .

:SystemTRU a rdfs:Class ;
    rdfs:label "System TRU"@en ;
    rdfs:comment "A traceable resource unit that represents a complete system composed of multiple component TRUs."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/hierarchy> ;
    rdfs:subClassOf :DesignedPhysicalSystem,
        :TraceableResourceUnit ;
    skos:definition "A TRU representing a system assembly with component traceability"@en ;
    skos:example "Assembled missile system with traceable components, manufactured engine assembly"@en .

:Transition a rdfs:Class ;
    rdfs:label "Transition"@en ;
    dcterms:source ammo:Transition ;
    rdfs:comment "A change from one state to another, occurring over a specific time interval and potentially triggered by events or conditions."@en ;
    rdfs:isDefinedBy <https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/modules/design/transition> ;
