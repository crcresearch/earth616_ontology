    dcterms:created "2024-04-09"^^xsd:date ;
    dcterms:creator [ a sdo:Person ;
            rdfs:label "Charles F. Vardeman II" ;
            sdo:affiliation "University of Notre Dame" ;
            sdo:email "cvardema@nd.edu"^^xsd:anyURI ;
