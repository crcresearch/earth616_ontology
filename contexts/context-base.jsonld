{
    "@context": {
      // Core namespace definitions
      "dscdo": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/ont/",
      "schema": "https://schema.org/",
      "prov": "http://www.w3.org/ns/prov#",
      "skos": "http://www.w3.org/2004/02/skos/core#",
      "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
      "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#", 
      "owl": "http://www.w3.org/2002/07/owl#",
      "xsd": "http://www.w3.org/2001/XMLSchema#",
      "sh": "http://www.w3.org/ns/shacl#",
      "dcterms": "http://purl.org/dc/terms/",
      
      // References to modular context files - provided as metadata for documentation
      // These are not actual JSON-LD keywords but metadata terms for reference
      "documentContext": {
        "@id": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/context/context-document.jsonld",
        "description": "Context for SCDigitalDocument and related document concepts for supply chain documentation",
        "version": "0.1.3"
      },
      "eventContext": {
        "@id": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/context/context-event.jsonld",
        "description": "Context for SupplyChainEvent and event types like TransferEvent, TransformationEvent",
        "version": "0.1.3"
      },
      "identifierContext": {
        "@id": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/context/context-identifier.jsonld",
        "description": "Context for Identifier pattern, IdentifierType, and IdentifierEncoding concepts",
        "version": "0.1.3"
      },
      "workflowContext": {
        "@id": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/context/context-workflow.jsonld",
        "description": "Context for InformationExtractionWorkflow and IEPipeline concepts",
        "version": "0.1.3"
      },
      "roleContext": {
        "@id": "https://schema-earth616-ks18.blocks.simbachain.com/nd/scdoc/context/context-role.jsonld",
        "description": "Context for ParticipantRole, SupplyChainRole and related role concepts",
        "version": "0.1.3"
      },
      
      // Core RDFS/OWL vocabulary terms
      "label": {"@id": "rdfs:label"},
      "comment": {"@id": "rdfs:comment"},
      "subClassOf": {"@id": "rdfs:subClassOf", "@type": "@id"},
      "domain": {"@id": "rdfs:domain", "@type": "@id"},
      "range": {"@id": "rdfs:range", "@type": "@id"},
      "type": {"@id": "rdf:type", "@type": "@id"},
      "value": {"@id": "schema:value"},
      
      // SKOS vocabulary for annotations
      "definition": {"@id": "skos:definition"},
      "prefLabel": {"@id": "skos:prefLabel"},
      "example": {"@id": "skos:example"},
      "note": {"@id": "skos:note"},
      "inScheme": {"@id": "skos:inScheme", "@type": "@id"},
      "changeNote": {"@id": "skos:changeNote"},
      
      // PROV vocabulary for provenance
      "Activity": {"@id": "prov:Activity"},
      "Entity": {"@id": "prov:Entity"},
      "Agent": {"@id": "prov:Agent"},
      "wasGeneratedBy": {"@id": "prov:wasGeneratedBy", "@type": "@id"},
      "wasAssociatedWith": {"@id": "prov:wasAssociatedWith", "@type": "@id"},
      "wasDerivedFrom": {"@id": "prov:wasDerivedFrom", "@type": "@id"},
      "used": {"@id": "prov:used", "@type": "@id"},
      
      // SHACL vocabulary for shapes/validation
      "targetClass": {"@id": "sh:targetClass", "@type": "@id"},
      "property": {"@id": "sh:property", "@type": "@id"},
      "path": {"@id": "sh:path", "@type": "@id"},
      "minCount": {"@id": "sh:minCount", "@type": "xsd:integer"},
      "maxCount": {"@id": "sh:maxCount", "@type": "xsd:integer"},
      "datatype": {"@id": "sh:datatype", "@type": "@id"},
      "nodeKind": {"@id": "sh:nodeKind", "@type": "@id"},
      "class": {"@id": "sh:class", "@type": "@id"},
      "shapesGraph": {"@id": "sh:shapesGraph", "@type": "@id"},
      
      // Dublin Core terms for metadata
      "created": {"@id": "dcterms:created", "@type": "xsd:date"},
      "modified": {"@id": "dcterms:modified", "@type": "xsd:date"},
      "creator": {"@id": "dcterms:creator"},
      "description": {"@id": "dcterms:description"},
      
      // Common DSCDO superclasses and patterns
      "Event": {"@id": "dscdo:Event"},
      "InformationObject": {"@id": "dscdo:InformationObject"},
      "ParticipantRole": {"@id": "dscdo:ParticipantRole"},
      "SpatioTemporalExtent": {"@id": "dscdo:SpatioTemporalExtent"},
      
      // Common DSCDO properties
      "hasSpatioTemporalExtent": {"@id": "dscdo:hasSpatioTemporalExtent", "@type": "@id"},
      "providesParticipantRole": {"@id": "dscdo:providesParticipantRole", "@type": "@id"},
      "hasInformationObject": {"@id": "dscdo:hasInformationObject", "@type": "@id"},
      "subEventOf": {"@id": "dscdo:subEventOf", "@type": "@id"},
      "assumesRole": {"@id": "dscdo:assumesRole", "@type": "@id"}
    }
  }